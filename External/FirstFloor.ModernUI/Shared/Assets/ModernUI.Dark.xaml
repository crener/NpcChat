<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="WindowBackgroundColor">#252526</Color>
    <Color x:Key="GeneralBackground">#333333</Color>
    <Color x:Key="GeneralText">#d1d1d1</Color>
    <Color x:Key="GeneralHover">#3e3e42</Color>

    <SolidColorBrush x:Key="ButtonBackground" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="ButtonBackgroundHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="ButtonBorder" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="ButtonBorderHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="ButtonBorderPressed" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="ButtonText" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ButtonTextHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ButtonTextPressed" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ButtonTextDisabled" Color="#515151" />

    <SolidColorBrush x:Key="DataGridBackground" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridForeground" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridCellBackground" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridCellBackgroundHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="DataGridCellBackgroundSelected" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="DataGridCellForeground" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridCellForegroundHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridCellForegroundSelected" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridHeaderBackground" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridHeaderBackgroundHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="DataGridHeaderBackgroundPressed" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="DataGridHeaderBackgroundSelected" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="DataGridHeaderForeground" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridHeaderForegroundHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridHeaderForegroundPressed" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridHeaderForegroundSelected" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="DataGridGridLines" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="DataGridDropSeparator" Color="{DynamicResource AccentColor}" />
    
    <SolidColorBrush x:Key="Hyperlink" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="HyperlinkHover" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="HyperlinkDisabled" Color="#717171" />

    <SolidColorBrush x:Key="InputBackground" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="InputBackgroundHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="InputBorder" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="InputBorderHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="InputText" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="InputTextHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="InputTextDisabled" Color="#717171" />
    
    <SolidColorBrush x:Key="ItemBackgroundHover" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="ItemBackgroundSelected" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="ItemBorder" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="ItemText" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ItemTextSelected" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ItemTextHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ItemTextDisabled" Color="#717171" />
    
    <SolidColorBrush x:Key="LinkButtonText" Color="#717171" />
    <SolidColorBrush x:Key="LinkButtonTextHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="LinkButtonTextPressed" Color="#515151" />
    <SolidColorBrush x:Key="LinkButtonTextDisabled" Color="#515151" />

    <SolidColorBrush x:Key="MenuText" Color="#515151" />
    <SolidColorBrush x:Key="MenuTextHover" Color="#919191" />
    <SolidColorBrush x:Key="MenuTextSelected" Color="{DynamicResource GeneralText}" />

    <SolidColorBrush x:Key="ModernButtonBorder" Color="#919191" />
    <SolidColorBrush x:Key="ModernButtonBorderHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ModernButtonBorderPressed" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ModernButtonBorderDisabled" Color="#515151" />
    <SolidColorBrush x:Key="ModernButtonIconBackgroundPressed" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="ModernButtonIconForegroundPressed" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ModernButtonText" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ModernButtonTextHover" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ModernButtonTextPressed" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ModernButtonTextDisabled" Color="#515151" />

    <SolidColorBrush x:Key="PopupBackground" Color="#252526" />

    <SolidColorBrush x:Key="ProgressBackground" Color="{DynamicResource GeneralBackground}" />

    <SolidColorBrush x:Key="ScrollBarBackground" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="ScrollBarThumbBackground" Color="Transparent" />
    <SolidColorBrush x:Key="ScrollBarThumbBackgroundDragging" Color="Transparent" />
    <SolidColorBrush x:Key="ScrollBarThumbBackgroundHover" Color="Transparent" />
    <SolidColorBrush x:Key="ScrollBarThumbBorder" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="ScrollBarThumbForeground" Color="#717171" />
    <SolidColorBrush x:Key="ScrollBarThumbForegroundDragging" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="ScrollBarThumbForegroundHover" Color="#919191" />
    
    <SolidColorBrush x:Key="SeparatorBackground" Color="{DynamicResource GeneralHover}" />

    <SolidColorBrush x:Key="SliderSelectionBackground" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="SliderSelectionBorder" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="SliderThumbBackground" Color="#717171" />
    <SolidColorBrush x:Key="SliderThumbBackgroundDragging" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="SliderThumbBackgroundHover" Color="#919191" />
    <SolidColorBrush x:Key="SliderThumbBackgroundDisabled" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="SliderThumbBorder" Color="#717171" />
    <SolidColorBrush x:Key="SliderThumbBorderDragging" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="SliderThumbBorderHover" Color="#919191" />
    <SolidColorBrush x:Key="SliderThumbBorderDisabled" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="SliderTrackBackground" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="SliderTrackBorder" Color="{DynamicResource GeneralBackground}" />
    <SolidColorBrush x:Key="SliderTick" Color="#717171" />
    <SolidColorBrush x:Key="SliderTickDisabled" Color="{DynamicResource GeneralBackground}" />

    <SolidColorBrush x:Key="SubMenuText" Color="#717171" />
    <SolidColorBrush x:Key="SubMenuTextHover" Color="#515151" />
    <SolidColorBrush x:Key="SubMenuTextSelected" Color="{DynamicResource GeneralText}" />

    <SolidColorBrush x:Key="WindowBackground" Color="{StaticResource WindowBackgroundColor}" />
    <SolidColorBrush x:Key="WindowBorder" Color="{DynamicResource AccentColor}" Opacity=".5" />
    <SolidColorBrush x:Key="WindowBorderActive" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="WindowHeaderGradient" Color="Transparent" />
    <SolidColorBrush x:Key="WindowText" Color="#c1c1c1" />
    <SolidColorBrush x:Key="WindowTextReadOnly" Color="#717171" />

    <SolidColorBrush x:Key="CloseButtonBackground" Color="{StaticResource WindowBackgroundColor}"  Opacity="0" />
    <SolidColorBrush x:Key="CloseButtonBackgroundOnMoseOver" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="CloseButtonForegroundOnMoseOver" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="CloseButtonBackgroundIsPressed" Color="#e51400" />
    <SolidColorBrush x:Key="CloseButtonForegroundIsPressed" Color="White" />

    <SolidColorBrush x:Key="SystemButtonBackground" Color="{StaticResource WindowBackgroundColor}"  Opacity="0" />
    <SolidColorBrush x:Key="SystemButtonBackgroundOnMoseOver" Color="{DynamicResource GeneralHover}" />
    <SolidColorBrush x:Key="SystemButtonForegroundOnMoseOver" Color="{DynamicResource GeneralText}" />
    <SolidColorBrush x:Key="SystemButtonBackgroundIsPressed" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="SystemButtonForegroundIsPressed" Color="{DynamicResource GeneralText}" />

    <Rectangle x:Key="WindowBackgroundContent" x:Shared="false" />
</ResourceDictionary>