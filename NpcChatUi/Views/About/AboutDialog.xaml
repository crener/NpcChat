<mui:ModernWindow x:Class="NpcChat.Views.Dialogs.AboutDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NpcChat.Views.Dialogs"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        mc:Ignorable="d"
        Title="About" Height="450" Width="800"
        ContentSource="/Views/About/AboutPage.xaml"
        >
    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="About">
            <mui:LinkGroup.Links>
                <mui:Link Source="/Views/About/AboutPage.xaml"></mui:Link>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup DisplayName="Licenses">
            <mui:LinkGroup.Links>
                <mui:Link Source="/Views/About/LicensePage.xaml"></mui:Link>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
